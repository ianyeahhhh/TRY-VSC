    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "events": {
    },
    "watch": [
        "index.js",
        "src/"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js, json"
}